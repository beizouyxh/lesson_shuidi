.index-container {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.index-container .logo {
    width: 452rpx;
    height: 196rpx;
    margin-top: 160rpx;
}

.index-container .username {
    margin-top: 100rpx;
    font-size: 20px;
    font-weight: 700;
}

.index-container .btn {
    border: 1px solid #109D59;
    margin-top: 300rpx;
    padding: 10px 20px;
    border-radius: 5px;
}

.index-container .btn text {
    color: #109D59;
}